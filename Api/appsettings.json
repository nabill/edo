{
  "AllowedHosts": "*",
  "AmazonS3": {
    "Options": "edo/amazon-s3/documents",
    "ContractsS3FolderName": "agency-contracts",
    "ImagesS3FolderName": "images"
  },
  "Authority": {
    "Options": "edo"
  },
  "ConnectionStrings": {
    "Edo": "Server={0};Port={1};Database=edo;Userid={2};Password={3}"
  },
  "CurrencyConverter": {
    "Options": "edo/currency-service"
  },
  "Edo": {
    "Booking": {
      "Options": "edo/booking-options"
    },
    "ConnectorClient": {
      "Options": "edo/client"
    },
    "Database": {
      "Options": "edo/connection-string"
    },
    "Email": {
      "Administrators": "administrators",
      "ApiKey": "apiKey",
      "CcNotificationAddresses": "ccNotificationAddresses",
      "EdoAgentAppFrontendUrl": "edoPublicUrl",
      "EdoManagementFrontendUrl": "edoManagementPublicUrl",
      "Options": "edo/email",
      "SenderAddress": "senderAddress",
      "ReservationsOfficeBackupEmail": "emailToSendCopy",
      "HappyTravelAccountsEmail": "happyTravelAccountsEmail"
    },
    "Google": {
      "Endpoint": "https://maps.googleapis.com/maps/api/",
      "Options": "edo/google"
    },
    "MapperClient": {
      "Options": "edo/mapper-client"
    },
    "MapperManagementClient":{
      "Options": "edo/mapper-management-client"
    },
    "Payfort": {
      "Options": "edo/payfort",
      "Urls": "edo/payfort/urls"
    },
    "NGenius": "edo/ngenius"
  },
  "Jaeger": {
    "AgentHost": "JAEGER_AGENT_HOST",
    "AgentPort": "JAEGER_AGENT_PORT"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "HappyTravel": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "PaymentLinks": {
    "Options": "edo/payment-links",
    "ServiceTypes": {
      "HTL": "Hotel Booking",
      "TRN": "Transfer",
      "CMS": "Custom service"
    },
    "Currencies": [
      "AED",
      "SAR",
      "USD"
    ],
    "Endpoint": "endpoint"
  },
  "Redis": {
    "Endpoint": "HTDC_REDIS_HOST"
  },
  "UrlGeneration": {
    "Options": "edo/url-generation"
  },
  "UserEvents": {
    "ElasticSearch": "edo/ElasticSearch"
  },
  "Vault": {
    "Endpoint": "HTDC_VAULT_ENDPOINT",
    "Engine": "secrets",
    "Role": "edo",
    "Token": "HTDC_VAULT_TOKEN"
  },
  "Telemetry": {
    "IsEnabled": false
  },
  "MongoDB": {
    "Options": "edo/mongodb"
  },
  "WideAvailabilityStorage": {
    "UseMongoDbStorage": true
  },
  "BankDetails": {
    "AccountDetails": {
      "AED": {
        "AccountNumber": "",
        "Iban": "",
        "BankName": "",
        "RoutingCode": "",
        "SwiftCode": ""
      },
      "EUR": {
        "AccountNumber": "",
        "Iban": "",
        "BankName": "",
        "RoutingCode": "",
        "SwiftCode": ""
      },
      "USD": {
        "AccountNumber": "",
        "Iban": "",
        "BankName": "",
        "RoutingCode": "",
        "SwiftCode": ""
      }
    },
    "BankAddress": "",
    "BankName": ""
  },
  "PaymentProcessor": {
    "EnabledPaymentProcessor": "Payfort"
  },
  "CreditCardProvider": "Vcc",
  "MarkupPolicyStorageOptions": {
    "Timeout": "0.00:00:10"
  }
}