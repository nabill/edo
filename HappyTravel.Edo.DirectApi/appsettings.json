{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "HappyTravel": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "Edo": "Server={0};Port={1};Database={2};Userid={3};Password={4}"
  },
  "Database": {
    "Options": "edo/connection-string"
  },
  "Authority": {
    "Options": "edo"
  },
  "Vault": {
    "Endpoint": "HTDC_VAULT_ENDPOINT",
    "Engine": "secrets",
    "Role": "edo",
    "Token": "HTDC_VAULT_TOKEN"
  },
  "Redis": {
    "Endpoint": "HTDC_REDIS_HOST"
  },
  "Telemetry": {
    "IsEnabled": false
  },
  "CurrencyConverter": {
    "Options": "edo/currency-service"
  },
  "Edo": {
    "ConnectorClient": {
      "Options": "edo/client"
    }
  }
}
